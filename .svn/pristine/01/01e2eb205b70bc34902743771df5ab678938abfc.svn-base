/*一排一个列表*/
.wrap .zhubo-list #list.sort_lists ul{
	margin-top: 2.2rem;
	background-color: #fff;
}
.wrap .zhubo-list #list.sort_lists ul li{
	position: relative;
	height: 2.2rem;
	border-bottom: 1px #dad8d9 solid;
	border-color: rgba(218,216,217,0.5);
}
.wrap .zhubo-list #list.sort_lists ul li a{
	padding: 0.2rem 0.4rem;
	display: block;
}
.wrap .zhubo-list #list.sort_lists ul li img{
	display: inline-block;
	float: left;
	width: 1.8rem;
	height: 1.8rem;
	border-radius: 50%;
}
.wrap .zhubo-list #list.sort_lists ul li p.tit{
	position: absolute;
	left: 2.4rem;
	top: 0.2rem;
	height: 0.6rem;
	width: 60%;
	line-height: 0.6rem;
	color: #2c2c2c;
}
.wrap .zhubo-list #list.sort_lists ul li p .name{
	float: left;
	max-width:90%;
	font-size: 0.4rem;
	overflow: hidden;
	word-break: break-all;
	white-space:nowrap;
	text-overflow:ellipsis;
}
.wrap .zhubo-list #list.sort_lists ul li p .sex{
	display: inline-block;
    background-image: url(../img/girl.png);
    background-repeat: no-repeat;
    background-size: contain;
    vertical-align: middle;
    width: 0.4rem;
    height: 0.4rem;
    margin-left: 0.2rem;
}
.wrap .zhubo-list #list.sort_lists ul li p .sex.girl{
	background-image: url(../img/girl.png);
}
.wrap .zhubo-list #list.sort_lists ul li p .sex.boy{
	background-image: url(../img/boy.png);
}
.wrap .zhubo-list #list.sort_lists ul li p.tit2{
	position: absolute;
	left: 2.4rem;
	top: 0.8rem;
	height: 0.6rem;
	line-height: 0.6rem;
	color: #353535;
}
.wrap .zhubo-list #list.sort_lists ul li p.tit2 span{
	font-size: 0.3rem;
}
.wrap .zhubo-list #list.sort_lists ul li .list_tips{
	position: absolute;
	left: 2.4rem;
	top: 1.4rem;
	height: 0.6rem;
	line-height: 0.6rem;
	color: #fff;
	overflow: hidden;
}
.wrap .zhubo-list #list.sort_lists ul li .list_tips i{
	position: relative;
	margin-right: 0.4rem;
	padding: 0 0.15rem;
	display: inline-block;
	float: left;
	font-style: normal;
	height: 0.5rem;
	line-height: 0.5rem;
	border-radius:3px 0 0 3px;
}
.wrap .zhubo-list #list.sort_lists ul li .list_tips i:before{
	content: '';
	position: absolute;
	right: -0.25rem;
	top: 0;
	display: inline-block;
	width: 0.25rem;
	height: 0.5rem;
	background-size: 100% auto;
	background-repeat: no-repeat;
	background-position: center;
}
.wrap .zhubo-list #list.sort_lists ul li .list_tips i:nth-of-type(1){
	background-color: #f67e3d;
}
.wrap .zhubo-list #list.sort_lists ul li .list_tips i:nth-of-type(2){
	background-color: #66e6f1;
}
.wrap .zhubo-list #list.sort_lists ul li .list_tips i:nth-of-type(3){
	background-color: #eacc01;
}
.wrap .zhubo-list #list.sort_lists ul li .list_tips i:nth-of-type(4){
	background-color: #c060ec;
}
.wrap .zhubo-list #list.sort_lists ul li .list_tips i:nth-of-type(1):before{
	background-image: url(../img/tip_icon1.png);
}
.wrap .zhubo-list #list.sort_lists ul li .list_tips i:nth-of-type(2):before{
	background-image: url(../img/tip_icon2.png);
}
.wrap .zhubo-list #list.sort_lists ul li .list_tips i:nth-of-type(3):before{
	background-image: url(../img/tip_icon3.png);
}
.wrap .zhubo-list #list.sort_lists ul li .list_tips i:nth-of-type(4):before{
	background-image: url(../img/tip_icon4.png);
}
.wrap .zhubo-list #list.sort_lists ul li .collect{
	/*display: none;*/
	position: absolute;
	right: 0;
	top: 0;
	width: 1.6rem;
	height: 2.2rem;
	text-align: center;
}
.wrap .zhubo-list #list.sort_lists ul li .collect i{
	margin-top: 0.5rem;
	display: block;
	width: 1.6rem;
	height: 0.75rem;
	background-position: center;
	background-repeat: no-repeat;
	background-size: 0.6rem auto;
	font-size: 0.3rem;
}
.wrap .zhubo-list #list.sort_lists ul li .collect i.on{
	background-image: url(../img/collect_on_icon.png);
}
.wrap .zhubo-list #list.sort_lists ul li .collect i.off{
	background-image: url(../img/collect_off_icon.png);
}
.wrap .zhubo-list #list.sort_lists ul li .collect p{
	font-size: 0.35rem;
	color: #ff8904;
}

/*图片模式*/
.wrap .zhubo-list #list.pic_sort ul{
	margin-top: 2.2rem;
	background-color: #f0f0f0;
	overflow: hidden;
}
.wrap .zhubo-list #list.pic_sort ul li{
	position: relative;
	margin-bottom: 2%;
	float: left;
	width: 50%;
	overflow:hidden;
	box-sizing: border-box;
	padding-right:1%;
}
.wrap .zhubo-list #list.pic_sort ul li:nth-child(even){
	padding-right:0;
	padding-left:1%;
}
.wrap .zhubo-list #list.pic_sort ul li a{
	display:block; background:#fff;
}
.wrap .zhubo-list #list.pic_sort ul li img{
	width: 100%;
	
}
.wrap .zhubo-list #list.pic_sort ul li .tit{
	padding-left: 0.2rem;
	height: 0.8rem;
	line-height: 0.8rem;
}
.wrap .zhubo-list #list.pic_sort ul li .tit .name{
	float: left;
	font-size: 0.4rem;
	color: #2c2c2c;
	max-width: 70%;
	overflow: hidden;
	word-break: break-all;
	white-space:nowrap;
	text-overflow:ellipsis;
}
.wrap .zhubo-list #list.pic_sort ul li .tit i.sex{
	display: inline-block;
    background-image: url(../img/girl.png);
    background-repeat: no-repeat;
    background-size: contain;
    vertical-align: middle;
    width: 0.4rem;
    height: 0.4rem;
    margin-left: 0.2rem;
}
.wrap .zhubo-list #list.pic_sort ul li .tit i.sex.boy{
	background-image: url(../img/boy.png);
}
.wrap .zhubo-list #list.pic_sort ul li .tit2{
	padding-left: 0.2rem;
	padding-bottom: 0.2rem;
	font-size: 0.3rem;
	color: #353535;
}
.wrap .zhubo-list #list.pic_sort ul li .tit2 .age{
	display: block;
}
.wrap .zhubo-list #list.pic_sort ul li .list_tips{
	display: none;
}
.wrap .zhubo-list #list.pic_sort ul li .collect{
	display: none;
	position: absolute;
	right: 0;
	bottom: 0.4rem;
	width: 1.2rem;
	height: 1.2rem;
	text-align: center;
}
.wrap .zhubo-list #list.pic_sort ul li .collect i{
	display: block;
	width: 1.2rem;
	height: 0.75rem;
	background-position: center;
	background-repeat: no-repeat;
	background-size: 0.6rem auto;
}
.wrap .zhubo-list #list.pic_sort ul li .collect i.on{
	background-image: url(../img/collect_on_icon.png);
}
.wrap .zhubo-list #list.pic_sort ul li .collect i.off{
	background-image: url(../img/collect_off_icon.png);
}
.wrap .zhubo-list #list.pic_sort ul li .collect p{
	font-size: 0.35rem;
	color: #ff8904;
}




.wrap .zhubo-list #list.pic_sort .getmore{
	float: left;
	width: 100%;
}
.getmore{
	margin-top: 0.2rem;
}

/*没有匹配内容提示*/
.wrap .zhubo-list .sorry{
	margin-top: 2.6rem;
}
.wrap .zhubo-list .sorry img{
	margin: 0 auto;
	display: block;
	width: 40%;
}
.wrap .zhubo-list .sorry p{
	margin-top: 0.4rem;
	color: #666;
	font-size: 0.35rem;
	text-align: center;
}

.res_btn{
	margin: 0.2rem auto 0 auto;
	width: 35%;
	height: 0.8rem;
	line-height: 0.8rem;
	text-align: center;
	border:1px #FF8904 solid;
	border-radius: 4px;
	color: #FF8904;
	font-size: 0.35rem;
	cursor: pointer;
}