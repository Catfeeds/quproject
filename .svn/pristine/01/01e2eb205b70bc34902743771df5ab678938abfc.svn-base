.sort_top input{
	margin-left: 0.6rem;
	padding-left: 0.7rem;
    box-sizing: border-box;
    width: 80%;
    height: 0.7rem;
    border-radius: 4px;
    background-color: #EFEFF4;
    border: none;
    font-size: 0.35rem;
    margin-top: 0.1rem;
    color: #333;
    background-image: url(../img/search_icon.png);
    background-repeat: no-repeat;
    background-size: 0.3rem auto;
    background-position: 0.2rem center;
    box-sizing: border-box;
}
.sort_top .list_type{
	float: right;
	display: inline-block;
	width: 1rem;
	height: 1rem;
	background-image: url(../img/sort_type1.png);
	background-position: center;
	background-repeat: no-repeat;
	background-size: 0.6rem auto;
}
.sort_top .list_type.pic{
	background-image: url(../img/sort_type2.png);
}
.wrap .zhubo-list .sort_nav{
	position: fixed;
	top: 1rem;
	box-sizing: border-box;
	width: 100%;
	height: 1rem;
	line-height: 1rem;
	background-color: #fff;
	border-bottom: 1px #eee solid;
	z-index: 99;
}
.wrap .zhubo-list .sort_nav li.active{
	border:1px #FF8904 solid;
	color: #FF8904
}
.wrap .zhubo-list .sort_nav li.on{
	color: #ff8904;
}
.wrap .zhubo-list .sort_bg{
	display: none;
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,0.4);
	z-index: 9;
}.wrap .zhubo-list .sort_nav li.on i{
	background-image: url(../img/choose_up_icon.png);
}
.wrap .zhubo-list .sort_nav li{
	position: relative;
	margin: 0.15rem 2.5%; 
	float: left;
	box-sizing: border-box;
	width: 20%;
	height: 0.7rem;
	line-height: 0.7rem;
	font-size: 0.35rem;
	color: #646464;
	text-align: center;
	overflow: hidden;
	word-break: break-all;
	white-space:nowrap;
	text-overflow:ellipsis;
    border-radius: 5px;
}

.wrap .zhubo-list .sort_nav li.current{
	border:1px #FF8904 solid;
	color: #FF8904;
}
.wrap .zhubo-list .sort_nav li i{
	display: inline-block;
	position: absolute;
	right: 0;
	top: 0;
	width: 0.4rem;
	height: 0.7rem;
	background-image: url(../img/choose_down_icon.png);
	background-repeat: no-repeat;
	background-size: 0.3rem auto;
	background-position: center;
}

.wrap .zhubo-list .sort_nav li i{
	right: 0.25rem;
}
.wrap .zhubo-list .sort_list{
	display: none;
	position: fixed;
	top: 1rem;
	left: 0;
	margin-top: 1rem;
	width: 100%;
	background-color: #fff;
	z-index: 10;
}
.wrap .zhubo-list .sort_list li{
	margin: 0 0.4rem;
	box-sizing: border-box;
	height: 1rem;
	line-height: 1rem;
	color: #646464;
	border-bottom: 1px #dfdfdf solid;
}
.wrap .zhubo-list .sort_list li i{
	float: right;
	display: none;
	line-height: 1rem;
	font-size: 0.4rem;
}
.wrap .zhubo-list .sort_list li.current i{
	display: inline-block;
}
.wrap .zhubo-list .sort_list .btn_box{
	width: 100%;
	height: 0.8rem;

}
.wrap .zhubo-list .sort_list .btn_l,
.wrap .zhubo-list .sort_list .btn_r{
	width: 50%;
	height: 1rem;
	text-align: center;
	line-height: 1rem;
	font-size: 0.35rem;
	border-width: 1px 0 0 0;
	border-style: solid;
}
.wrap .zhubo-list .sort_list .btn_l{
	border-color: #dfdfdf;
}
.wrap .zhubo-list .sort_list .btn_r{
	border-color: #FF8904;
}
.wrap .zhubo-list .sort_list div.btn_l{
	float: left;
	color: #666;
	background-color: #fff;
}
.wrap .zhubo-list .sort_list div.btn_r{
	float: right;
	color: #fff;
	background-color: #FF8904
}

.wrap .zhubo-list .sort_list li.current{
	color: #ff8904;
	border-bottom: 1px #ff8904 solid;
}
.wrap .zhubo-list .tips_sort{
	height: 8rem;
	overflow-y: auto;
}
.wrap .zhubo-list .tips_sort li:last-of-type{
	margin-bottom: 1rem;
}
.wrap .zhubo-list .tips_sort .btn_box{
	position: fixed;
    top: 9rem;
}
.wrap .zhubo-list .platform_sort li:last-of-type{
	margin-bottom: 1rem;
}
.wrap .zhubo-list .city_sort #city{
	float: left;
	width: 100%;
	height: 8rem;
	overflow-y: auto;
}
.wrap .zhubo-list .city_sort .city_box{
	position: relative;
	width: 100%;
	height: 8rem;
}
.wrap .zhubo-list .city_sort #city label{
	display: inline-block;
	padding-left: 0.4rem;
	box-sizing: border-box;
	height: 1rem;
	width: 100%;
	line-height: 1rem;
	font-size: 0.3rem;
	color: #646464;
	border-bottom: 1px #dfdfdf solid;
}
.wrap .zhubo-list .city_sort #city input[type='radio']{
	display: none;
}
.wrap .zhubo-list .city_sort #city input[type='radio']:checked+label{
	background-color: #f0f0f0;
}
.wrap .zhubo-list .city_sort .check_city{
	display: none;
	position: absolute;
	right: 0;
	top: 0;
	width: 50%;
	height: 8rem;
	overflow-y: auto;
	background-color: #f0f0f0;
}
.wrap .zhubo-list .city_sort .check_city label.check_on{
	padding-left: 0.4rem;
	display: inline-block;
	box-sizing: border-box;
	width: 100%;
	height: 1rem;
	line-height: 1rem;
	font-size: 0.3rem;
	color: #646464;
	border-bottom: 1px #dfdfdf solid;
	background-color: #f0f0f0;
	cursor:pointer;
}
.wrap .zhubo-list .city_sort .check_city label.check_on.current{
}
.wrap .zhubo-list .city_sort .check_city span.check_off{
	margin: 0.2rem 30% 0.2rem 0;
	display: inline-block;
	width: 60%;
	height: 0.8rem;
	line-height: 0.8rem;
	color: #333;
	font-size: 0.3rem;
	text-align: center;
}
