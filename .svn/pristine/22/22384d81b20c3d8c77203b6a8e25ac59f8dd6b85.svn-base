html,body{
	height: 100%;
}
.wrap .changeMsg-wrap .top-link{
	padding: 0.4rem;
	box-sizing: border-box;
	width: 100%;
	background-color: #fff;
}
.wrap .changeMsg-wrap .top-link span{
	display: inline-block;
	height: 0.6rem;
	line-height: 0.6rem;
	font-size: 0.4rem;
	color: #7b7b7b;
}
.wrap .changeMsg-wrap .top-link span:last-of-type{
	float: right;
}
.wrap .changeMsg-wrap .top-link span:last-of-type:after{
	content: '';
	display: inline-block;
	width: 0.3rem;
	height: 0.3rem;
	border-top: 1px #7b7b7b solid;
	border-right: 1px #7B7B7B solid;
	transform: rotate(45deg);
}
