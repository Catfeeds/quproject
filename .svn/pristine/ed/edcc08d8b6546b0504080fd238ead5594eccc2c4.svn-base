.wrap .detail_wrap .detail_btn .ir a.signup{	background-color:#FF8904;}
.wrap .detail_wrap .detail_btn .ir a.off{	background-color:#ccc;}
.wrap .detail_wrap .detail_btn .ir a.tel{	background-color:#ffc004}
.wrap .detail_wrap .detail_btn .il a.on .fa:before {    content: "\f004";}
.wrap .detail_wrap .detail_btn .il a.off .fa:before {    content: "\f08a";}
.wrap .detail_wrap .detail_btn .ir a.release {    background-color:#FF8904 ;	color: #fff;	width:100%;}

/*åŠ¨ç”»*/
@-webkit-keyframes bounceIn {
    0% {
        opacity:1;
        -webkit-transform:scale(1)
    }
   
    20% {
        opacity:1;
        -webkit-transform:scale(1.1)
    }
 
    100% {
        -webkit-transform:scale(1)
    }
}
@-moz-keyframes bounceIn {
    0% {
        opacity:1;
        -moz-transform:scale(1)
    }
  
    20% {
        opacity:1;
        -moz-transform:scale(1.1)
    }
   
    100% {
        -moz-transform:scale(1)
    }
}
.bounceIn {
     -webkit-animation-name:bounceIn;
     -moz-animation-name:bounceIn
 }
.detail_top_pic{
	-webkit-animation: bounceIn 8s infinite;
}




