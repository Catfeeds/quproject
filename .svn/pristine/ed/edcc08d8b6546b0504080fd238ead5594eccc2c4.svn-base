#loadactivity{ display:block;background:#efeff4;width:100%;height:100%;padding-top:60%;text-align: center;position:fixed;z-index:999;top:2rem;}
.activity-index .nav-screen{ position:fixed;width:100%;height:1rem;z-index:98;border-bottom: 1px #eee solid;}
.activity-index .nav-screen ul{width:100%;height:1rem;line-height:1rem;text-align:center;background:#fff;display: -webkit-box;display: -moz-box;display: -ms-box;display: -o-box;}
.activity-index .nav-screen ul li{width:25%}
.activity-index .nav-screen li span {color:#666;font-size:0.35rem;line-height:1rem;vertical-align:middle;}
.activity-index .nav-screen li i { color:#666; font-size:0.45rem; padding-left:0.1rem; line-height:1rem; vertical-align:middle; }
.activity-index .nav-screen li.active span{ color:#FF8904; }
.activity-index .nav-screen li.active .fa-angle-down:before { content: "\f106"; color:#FF8904; }
.activity-index .dropdown-wrapper { display:none; position: fixed; top: 2rem; width: 100%; height:100%; z-index: 97;}
.activity-index .shade{ height: 100%; width:100%; background: #333; filter:alpha(opacity=50); -moz-opacity:0.5; opacity:0.5; position: absolute; z-index: 1;}
.activity-index .dropdown-scroller { width: 100%; background: #fff; position:relative; z-index:2;}
.activity-index .business-wrapper,.status-wrapper,.sort-wrapper{ width:100%;display:none; }
.activity-index .business-wrapper.active,.status-wrapper.active,.sort-wrapper.active{ width:100%;display:block; }
.activity-index .dropdown-list li{ height:1rem;line-height:1rem;margin:0 0.4rem; }

.activity-index .dropdown-list>li{ border-bottom: 1px solid #eee; }
.activity-index .dropdown-list li.active{ border-bottom:1px solid #FF8904; color:#FF8904; }
.activity-index .dropdown-list li.active i{ display:block; vertical-align:middle; line-height:1rem;font-size: 0.4rem;}
.activity-index .dropdown-list li i{ float: right;display:none;}

.index_top{ position: fixed; z-index: 99; width: 100%; top: 0px; height: 1rem; background-color: #fff; border-bottom: 1px solid #e8e8e8; line-height: 1rem;    text-align: center;    font-size: 0.4rem;    color: #333;display: box;display: -webkit-box;display: -moz-box;}
.index_top .location { padding-left: 0.2rem;	color:#333;	font-size:0.4rem; white-space:nowrap;}
.index_top .location span{ color:#333;	font-size:0.4rem; vertical-align: middle;}
.index_top .location i{ padding-left: 0.1rem; vertical-align:middle; font-size:0.5rem;}
.index_top .index_search{ -webkit-box-flex: 1; -webkit-flex: 1; flex: 1;}
.index_top .index_menu{ margin-right:0.2rem;}
.index_top .index_menu i{ line-height:1rem; font-size: 0.5rem; color: #999; }
.index_top input { padding-left: 0.7rem; box-sizing: border-box; width: 85%; height: 0.7rem; border-radius: 4px; background-color: #EFEFF4; border: none; font-size: 0.35rem; margin-top:0.1rem; color: #333; background-image: url(../img/search_icon.png); background-repeat: no-repeat; background-size: 0.3rem auto; background-position: 0.2rem center; box-sizing: border-box;}

