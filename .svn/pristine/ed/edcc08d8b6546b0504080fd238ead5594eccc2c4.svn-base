.wrap .list-wrap .type_box .type{	position: relative;	width: 100%;}
.wrap .list-wrap .type_box .type img{	display: block;	width: 100%;}
.wrap .list-wrap .sort_list{	width: 100%;	height: 1rem;	line-height: 1rem;	background-color: #fff;    box-shadow: 0 1px 3px #d4cece;}
.wrap .list-wrap .sort_list ul{	float: left;	width: 100%;	height: 1rem;	line-height: 1rem;}
.wrap .list-wrap .sort_list ul li{	float: left;	width: 25%;	height: 1rem;	line-height: 1rem;	text-align: center;	font-size: 0.35rem;	color: #333;	position:relative;	z-index:1;}
.wrap .list-wrap .sort_list ul li.current{	position: relative;	color:  #FF8904;}
.wrap .list-wrap .sort_list ul li.current:after{	content: "";    position: absolute;    bottom: 0;    left: 20%;	width: 60%;    height: 0px;	border-bottom: 1px solid #FF8904;}
.wrap .list-wrap .sort_list .time li.current i{	color:#FF8904;}
.wrap .list-wrap .sort_list .time.start,.wrap .list-wrap .sort_list .time.end{	color: #000;}
.wrap .list-wrap .sort_list .time.start i{	position: absolute;	top:50%;	transform: translateY(-50%);	-webkit-transform: translateY(-50%);	-moz-transform: translateY(-50%);	-ms-transform: translateY(-50%);	color: #000;}
.wrap .list-wrap .sort_list .time.end i{	position: absolute;	top:50%;	transform: translateY(-50%);	-webkit-transform: translateY(-50%);	-moz-transform: translateY(-50%);	-ms-transform: translateY(-50%);	color: #000;}
.wrap .list-wrap .sort_list .time i{	color: #666;	font-size: 0.45rem;}
.wrap .list-wrap .sort_list .time .fa-caret-up{	position: absolute;	right: 0.1rem;	top: 0.15rem;}
.wrap .list-wrap .sort_list .time .fa-caret-down{	position: absolute;	right: 0.1rem;	bottom: 0.15rem;}
