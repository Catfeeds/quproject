.attachment a.attent {
    float: right;
    font-style: normal;
    font-size: 0.3rem;
    /*color: #FF8904;*/
    line-height: 0.5rem;
    padding: 0.1rem 0.3rem;
    display: inline-block;
    text-align: center;
    border-radius: 5%;
    margin-top: 0.15rem;
}

.swiper-pagination{
	width: 100%;
	height: 1rem;
	display: -webkit-box;
	display: -moz-box;
	display: -ms-box;
	display: -o-box;
	position: relative !important;
	background-color: #fff;
	/*border-bottom:0.1rem solid #efeff4;*/
	z-index: 98;
}
.swiper-pagination li{
    position: relative;
	-moz-box-flex:1;
	-ms-box-flex:1;
	-o-box-flex:1;
	-webkit-box-flex:1;
	box-flex:1;
	text-align: center;
	height:1rem;
	line-height:1rem;
	font-size: 0.35rem;
	color: #333;
	border-radius: 0;
	background-color: #fff;
}
.swiper-pagination li a{
    display:block;
	color: #333;
}
.swiper-pagination li.current{
    color: #FF8904 !important;
	box-sizing: border-box;
}
.swiper-pagination li.current a{
    color: #FF8904 !important;
}
.swiper-pagination li.current:after {
    content: "";
    position: absolute;
    bottom: 0;
    left: 20%;
	width:60%;
    box-sizing: border-box;
    height: 0px;
    border-bottom: 1px solid #FF8904;
}
.swiper-pagination .swiper-pagination-bullet{
	opacity: 1 !important;
	/*border-bottom: 1px #ccc solid;*/
	box-sizing: border-box;
	display:block;
	
}
.swiper-pagination .swiper-pagination-bullet-active{
	color: #FF8904 !important;
	/*border-bottom: 0.05rem #FF8904 solid;*/
	box-sizing: border-box;
}
.swiper-pagination .swiper-pagination-bullet-active:after {
    content: "";
    position: absolute;
    bottom: 0;
    left: 20%;
	width:60%;
    box-sizing: border-box;
    height: 0px;
    border-bottom: 1px solid #FF8904;
}

.activity_close{
	position: absolute;
    display: block;
	width:1.4rem;
	line-height:0.6rem;
	text-align:center;
    color: #fff;
	background:#FF8904;
    font-size: 0.35rem;
    border-radius: 5%;
    opacity: 1;
	left:50%;
	top:50%;
	margin-top:-0.3rem;
	margin-left:-0.7rem;
}
ul li .status_btn{
	position: absolute;
	left: 0.3rem;
	top: 0.3rem;
	display: inline-block;
	padding: 2px 10px;
	color: #fff;
	font-size: 0.3rem;
	border-radius: 4px;
	opacity: 0.7;
}
ul li .status_btn.end{
	background-color: #666;
}
