}
.headerfull{
	width: 100%;
	height: 1rem;
	overflow:hidden;
}
.header .location{
	position: absolute;
    left: 0.4rem;
	display: block;
	width:30%;
}
.header .location span{
	font-size: 0.4rem;
	line-height: 1rem;
	color: #333;
	float:left;
}
.header .location i{
	padding-left: 0.1rem;
	font-size:0.5rem;
	color:#333;
	float:left;
	line-height:1rem;
}
.header .menu{
	position: absolute;
    right: 0;
	top:0px;
	line-height:1rem;
	font-size:0.5rem;
	color:#666;
	padding-right:0.2rem;
	width:20%;
	text-align: right;
}
.header .menu i{
	display: block;
	line-height:1rem;
	font-size:0.5rem;
	color:#999;
	padding:0 0.2rem;
}
	.header .goback{
		color: #333;
		left:0px;
		    /*top: 0.3rem;*/
		padding: 0 0.4rem;
		display: block;
		position:absolute;
		font-size: 0.4rem;
    }
	.header .goback i{
		padding-right:0.1rem;
		font-size:0.75rem;
		color:#666;
		line-height:1rem;
		height:1rem;
		float:left;
		display:block;
    }
	.header .goback p{
		display: block;
		float:left;
		height:1rem;
		line-height:1rem;
		font-size:0.4rem;
	}
	.header .goback p:before{
		content: '';
		display: inline-block;
		width: 0.3rem;
		height: 0.3rem;
		border-width: 2px 2px 0 0;
		border-style: solid;
		border-color: #666;
		transform: rotate(-135deg);
		-webkit-transform: rotate(-135deg);
		-moz-transform: rotate(-135deg);
		-ms-transform: rotate(-135deg);
		vertical-align:middle;
	}
	.header p.header_title{
		width:60%;
		overflow: hidden;
		text-overflow: ellipsis;
		white-space: nowrap;
		text-align:center;
		padding-left: 20%;
		font-size: 0.4rem;
	}
	
