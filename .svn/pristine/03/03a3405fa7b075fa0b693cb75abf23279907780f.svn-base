.ux-popmenu {
    display: none;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1001;
    transition: background-color .2s ease-in-out;
    overflow: hidden;
}
.ux-popmenu .content {
    position: absolute;
